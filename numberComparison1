	public static void main (String args[]){
		
		numberCompare(1, 34, 45);
	}
	
	public static void numberCompare(int x, int y, int z){
		if (x > y){
			if (x > z){
				System.out.printf("The maximum number is %d.", x);
			}
			else
				System.out.printf("The maximum number is %d.", z);
		}
		else{
			if (y > z){
				System.out.printf("The maximum number is %d.", y);
			}
			else
				System.out.printf("The maximum number is %d.", z);
		}
	}
