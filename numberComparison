	public static void main (String args[]){
		numberCompare(200, 29, 9);
	}
	
	public static void numberCompare(int x, int y, int z){
		if (x > y && x > z){
			System.out.printf("The maximum number is %d.\n", x);
		}
		
		if (y > x && y > z){
			System.out.printf("The maximum number is %d.\n", y);
		}
		
		if (z > x && z > y){
			System.out.printf("The maximum number is %d.\n", z);
		}
	}
