public static void main(String args[]){

    int x = 13;
    int y = 34;

    System.out.println("Before X = " + x);
    System.out.println("Before Y = " + y);

    //利用差值
    y = x - y;
    x = x - y;
    y = x + y;

    System.out.println("After X = " + x);
    System.out.println("After Y = " + y);
}
